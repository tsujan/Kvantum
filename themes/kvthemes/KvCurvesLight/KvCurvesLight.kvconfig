[%General]
author=Tsu Jan
comment=A light, nont-flat theme with curved elements that supports translucency
x11drag=true
alt_mnemonic=true
left_tabs=false
attach_active_tab=false
mirror_doc_tabs=false
group_toolbar_buttons=true
spread_progressbar=true
progressbar_thickness=8
composite=true
menu_shadow_depth=6
tooltip_shadow_depth=6
splitter_width=7
scroll_width=12
scroll_min_extent=50
slider_width=6
slider_handle_width=16
slider_handle_length=16
center_toolbar_handle=true
check_size=16
menubar_mouse_tracking=true
toolbutton_style=0
double_click=false
translucent_windows=false
blurring=false
popup_blurring=true
opaque=kaffeine,kmplayer,subtitlecomposer,kdenlive,vlc,smplayer,smplayer2,avidemux,avidemux2_qt4,avidemux3_qt4,avidemux3_qt5,kamoso,QtCreator,VirtualBox,trojita,dragon,digikam
vertical_spin_indicators=false
inline_spin_indicators=true
combo_as_lineedit=true
combo_menu=true
merge_menubar_with_toolbar=false
scroll_arrows=false
transient_scrollbar=true
fill_rubberband=false
small_icon_size=16
large_icon_size=32
button_icon_size=16
toolbar_icon_size=16
tooltip_delay=-1
animate_states=true
button_contents_shift=true
groupbox_top_label=false
joined_inactive_tabs=true
layout_spacing=2
submenu_overlap=0

[GeneralColors]
window.color=#E5E5E6
base.color=#FBFBFB
alt.base.color=#F0F0F0
button.color=#E5E5E6
light.color=white
mid.light.color=#ECECEC
dark.color=#B4B4B4
mid.color=#D9D9D9
highlight.color=#5d86c5
inactive.highlight.color=#4271b8
text.color=black
window.text.color=black
button.text.color=black
disabled.text.color=#00000073
tooltip.text.color=white
highlight.text.color=white
link.color=#0057AE
link.visited.color=#452886
progress.indicator.text.color=white

[Hacks]
blur_translucent=true
transparent_dolphin_view=false
transparent_ktitle_label=true
transparent_menutitle=false
kcapacitybar_as_progressbar=true
force_size_grip=true
iconless_menu=false
iconless_pushbutton=false
normal_default_pushbutton=false
transparent_pcmanfm_sidepane=true
respect_darkness=false
disabled_icon_opacity=100
lxqtmainmenu_iconsize=22
single_top_toolbar=false
tint_on_mouseover=0
transparent_pcmanfm_view=false

[PanelButtonCommand]
frame=true
frame.element=button
focusFrame=true
frame.top=3
frame.bottom=3
frame.left=3
frame.right=3
frame.expanded.top=2
frame.expanded.bottom=2
frame.expanded.left=2
frame.expanded.right=2
interior=true
interior.element=button
indicator.size=10
text.normal.color=black
text.focus.color=#0052aa
text.press.color=black
text.toggle.color=black
text.shadow=0
text.margin=1
text.iconspacing=3
indicator.element=arrow
text.margin.top=2
text.margin.bottom=2
text.margin.left=6
text.margin.right=6
text.shadow.xshift=2
text.shadow.yshift=2
text.shadow.color=#000000
text.shadow.alpha=255
text.shadow.depth=1
frame.expansion=2.7font
min_height=+0.1font

[PanelButtonTool]
inherits=PanelButtonCommand

