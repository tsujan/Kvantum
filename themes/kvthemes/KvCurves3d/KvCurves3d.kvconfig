[%General]
author=Tsu Jan
comment=A dark, 3d theme with curved elements.\nIt supports window translucency with a rough-glass texture.
x11drag=true
alt_mnemonic=true
left_tabs=false
attach_active_tab=false
mirror_doc_tabs=false
group_toolbar_buttons=true
spread_progressbar=false
progressbar_thickness=12
composite=true
menu_shadow_depth=6
tooltip_shadow_depth=6
splitter_width=7
scroll_width=12
scroll_min_extent=50
slider_width=6
slider_handle_width=16
slider_handle_length=16
center_toolbar_handle=true
check_size=16
menubar_mouse_tracking=true
toolbutton_style=0
double_click=false
translucent_windows=false
blurring=false
popup_blurring=true
opaque=kaffeine,kmplayer,subtitlecomposer,kdenlive,vlc,smplayer,smplayer2,avidemux,avidemux2_qt4,avidemux3_qt4,avidemux3_qt5,kamoso,QtCreator,VirtualBox,trojita,dragon,digikam
vertical_spin_indicators=false
inline_spin_indicators=true
combo_as_lineedit=true
combo_menu=true
merge_menubar_with_toolbar=false
scroll_arrows=false
transient_scrollbar=true
transient_groove=true
fill_rubberband=false
small_icon_size=16
large_icon_size=32
button_icon_size=16
toolbar_icon_size=16
tooltip_delay=-1
animate_states=true
no_window_pattern=true
dark_titlebar=true

[GeneralColors]
window.color=#3D3D3E
base.color=#282828
alt.base.color=#323232
button.color=#3D3D3E
light.color=#606060
mid.light.color=#555555
dark.color=#171717
mid.color=#3c3c3c
highlight.color=#8c1011
inactive.highlight.color=#730d0e
tooltip.base.color=black
text.color=white
window.text.color=white
button.text.color=white
disabled.text.color=#A0A0A0
tooltip.text.color=white
highlight.text.color=white
link.color=#2EB8E6
link.visited.color=#FF6666

[Hacks]
respect_darkness=true
blur_translucent=true
transparent_dolphin_view=false
transparent_ktitle_label=true
transparent_menutitle=false
kcapacitybar_as_progressbar=true
force_size_grip=true
iconless_menu=false
iconless_pushbutton=false
normal_default_pushbutton=false
transparent_pcmanfm_sidepane=true
lxqtmainmenu_iconsize=22

[PanelButtonCommand]
frame=true
frame.element=button
focusFrame=true
frame.top=3
frame.bottom=3
frame.left=3
frame.right=3
frame.expanded.top=2
frame.expanded.bottom=2
frame.expanded.left=2
frame.expanded.right=2
interior=true
interior.element=button
indicator.size=10
text.normal.color=white
text.focus.color=#FFCA00
text.press.color=white
text.toggle.color=white
text.shadow=0
text.margin=1
text.iconspacing=3
indicator.element=arrow
text.margin.top=3
text.margin.bottom=3
text.margin.left=6
text.margin.right=6
text.shadow.xshift=2
text.shadow.yshift=2
text.shadow.color=#000000
text.shadow.alpha=255
text.shadow.depth=1
frame.expansion=2.8font
min_height=+0.1font

[PanelButtonTool]
inherits=PanelButtonCommand

[Dock]
inherits=PanelButtonCommand
frame=true
interior=true
frame.top=2
frame.bottom=2
frame.left=2
frame.right=2

[DockTitle]
inherits=PanelButtonCommand
frame=false
interior=false
text.focus.color=white
text.bold=true
text.margin.top=2
text.margin.bottom=2
text.margin.left=3
text.margin.right=3
frame.top=2
frame.bottom=2
frame.left=2
frame.right=2

[IndicatorSpinBox]
